{
  "name": "nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Paul Uhrynovych (Lauer) <lauer.dev@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "gulp": "gulp --require babel-register --gulpfile .tasks",
    "push": "gulp git_status --gulpfile .tasks/git.js && gulp git_add --gulpfile .tasks/git.js && gulp git_status --gulpfile .tasks/git.js && gulp git_commit --gulpfile .tasks/git.js && gulp git_push --gulpfile .tasks/git.js && gulp git_status --gulpfile .tasks/git.js",
    "ssh": "gulp status --gulpfile .tasks/ssh.js",
    "deploy": "npm run push && npm run ssh",
    "build": "gulp build --require babel-register --gulpfile .tasks && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "doc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "babelrc": false
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@mdi/font": "^2.4.85",
    "@nuxtjs/auth": "^4.5.1",
    "@nuxtjs/axios": "^5.3.1",
    "@vuedoc/md": "^1.2.2",
    "@vuedoc/parser": "^1.3.0",
    "consola": "^1.4.1",
    "cookie": "^0.3.1",
    "gulp-cli": "^2.0.1",
    "install": "^0.11.0",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "npm": "^6.1.0",
    "nuxt": "^1.0.0",
    "perfect-scrollbar": "^1.4.0",
    "svg-inline-loader": "^0.8.0",
    "vue-awesome-swiper": "^3.1.3",
    "vue-focus": "^2.1.0",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-slick": "^1.1.12",
    "vue-svg-filler": "^1.0.6",
    "vue2-animate": "^2.0.0",
    "vuejs-logger": "^1.4.0",
    "vuex-shared-mutations": "0.0.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "latest",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "latest",
    "babel-preset-stage-3": "latest",
    "body-parser": "^1.18.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cookie-parser": "^1.4.3",
    "del": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "fs": "0.0.1-security",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "latest",
    "gulp-cache": "latest",
    "gulp-changed": "latest",
    "gulp-filesize": "latest",
    "gulp-git": "latest",
    "gulp-if": "latest",
    "gulp-imagemin": "latest",
    "gulp-notify": "latest",
    "gulp-plumber": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-ssh": "latest",
    "gulp-useref": "latest",
    "gulp-watch": "latest",
    "gutil-color-log": "latest",
    "imagemin-pngquant": "latest",
    "jsdoc": "latest",
