{
  "name": "nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Paul Uhrynovych (Lauer) <lauer.dev@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "gulp": "gulp --require babel-register --gulpfile .tasks",
    "push": "gulp git_status --gulpfile .tasks/git.js && gulp git_add --gulpfile .tasks/git.js && gulp git_status --gulpfile .tasks/git.js && gulp git_commit --gulpfile .tasks/git.js && gulp git_push --gulpfile .tasks/git.js && gulp git_status --gulpfile .tasks/git.js",
    "deploy": "gulp status --gulpfile .tasks/ssh.js",
    "build": "gulp build --require babel-register --gulpfile .tasks && nuxt build",
    "start": "nuxt start", 
    "generate": "nuxt generate",
    "doc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "babelrc": false
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@mdi/font": "^2.4.85",
    "@nuxtjs/auth": "^4.5.1",
    "@nuxtjs/axios": "^5.3.1",
    "@vuedoc/md": "^1.2.2",
    "@vuedoc/parser": "^1.3.0",
    "consola": "^1.4.1",
    "cookie": "^0.3.1",
